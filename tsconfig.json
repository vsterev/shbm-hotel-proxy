{
  "compilerOptions": {
    "outDir":"dist",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "target": "es2022",
    "module": "commonjs",
    "esModuleInterop": true,
    "incremental":true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "exclude": [
    "dist",
    "build",
    "node_modules"
  ],
}